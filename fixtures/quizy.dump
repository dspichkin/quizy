INSERT INTO "quizy_lesson" VALUES(47,'2aad90544d7811e58bca03426b361ead','2015-08-28 07:29:44.536086','2015-10-20 08:15:16.176677',1,'IELTS letters (1)',0,'<b>You have several library books that you are unable to return because a family member living in another city has fallen ill and you must go and take care of him/her.&nbsp;</b><p><span style="line-height: 1.45em; background-color: initial;"><br></span></p><p><span style="line-height: 1.45em; background-color: initial;">Write a letter to the library explaining the situation. In your letter</span><br></p><p></p><p></p><ul><li><span style="line-height: 1.45em; background-color: initial;">apologize for the inconvenience caused and</span></li><li><span style="line-height: 1.45em; background-color: initial;">explain what you will do to get your books returned.&nbsp;</span><br></li></ul><p></p><p>You should write at least 150 words.
</p><p></p>
','{}',1,'lessons/2aad9054/lesson_4948.png',1,10,'outside','1',1200,2,0,'lessons/2aad9054/media_6377.jpg');
